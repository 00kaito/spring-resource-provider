compile = "javac -encoding UTF-8 -classpath $CLASSPATH -d target/classes $(find . -type f -name '*.java')"
run = "java -Dfile.encoding=UTF-8 -classpath target/classes:$CLASSPATH:target/dependency/* com.replit.Application"
entrypoint = "pom.xml"
hidden = ["**/*.class"]

[env]
SPRING_PROFILES_ACTIVE="prod"
M2_REPO="/home/runner/.m2/repository"
CLASSPATH="$M2_REPO/org/springframework/boot/spring-boot-starter-webflux/3.0.5/*:$M2_REPO/org/springframework/boot/spring-boot-starter/3.0.5/*:$M2_REPO/org/springframework/boot/spring-boot/3.0.5/*:$M2_REPO/org/springframework/boot/spring-boot-autoconfigure/3.0.5/*:$M2_REPO/org/springframework/boot/spring-boot-starter-logging/3.0.5/*:$M2_REPO/ch/qos/logback/logback-classic/1.4.6/*:$M2_REPO/ch/qos/logback/logback-core/1.4.6/*:$M2_REPO/org/apache/logging/log4j/log4j-to-slf4j/2.19.0/*:$M2_REPO/org/apache/logging/log4j/log4j-api/2.19.0/*:$M2_REPO/org/slf4j/jul-to-slf4j/2.0.7/*:$M2_REPO/jakarta/annotation/jakarta.annotation-api/2.1.1/*:$M2_REPO/org/yaml/snakeyaml/1.33/*:$M2_REPO/org/springframework/boot/spring-boot-starter-json/3.0.5/*:$M2_REPO/com/fasterxml/jackson/core/jackson-databind/2.14.2/*:$M2_REPO/com/fasterxml/jackson/core/jackson-annotations/2.14.2/*:$M2_REPO/com/fasterxml/jackson/core/jackson-core/2.14.2/*:$M2_REPO/com/fasterxml/jackson/datatype/jackson-datatype-jdk8/2.14.2/*:$M2_REPO/com/fasterxml/jackson/datatype/jackson-datatype-jsr310/*:$M2_REPO/com/fasterxml/jackson/module/jackson-module-parameter-names/2.14.2/*:$M2_REPO/org/springframework/boot/spring-boot-starter-reactor-netty/3.0.5/*:$M2_REPO/io/projectreactor/netty/reactor-netty-http/1.1.5/*:$M2_REPO/io/netty/netty-codec-http/4.1.90.Final/*:$M2_REPO/io/netty/netty-common/4.1.90.Final/*:$M2_REPO/io/netty/netty-buffer/4.1.90.Final/*:$M2_REPO/io/netty/netty-transport/4.1.90.Final/*:$M2_REPO/io/netty/netty-codec/4.1.90.Final/*:$M2_REPO/io/netty/netty-handler/4.1.90.Final/*:$M2_REPO/io/netty/netty-codec-http2/4.1.90.Final/*:$M2_REPO/io/netty/netty-resolver-dns/4.1.90.Final/*:$M2_REPO/io/netty/netty-resolver/4.1.90.Final/*:$M2_REPO/io/netty/netty-codec-dns/4.1.90.Final/*:$M2_REPO/io/netty/netty-transport-native-epoll/4.1.90.Final/*:$M2_REPO/io/netty/netty-transport-native-unix-common/4.1.90.Final/*:$M2_REPO/io/netty/netty-transport-classes-epoll/4.1.90.Final/*:$M2_REPO/io/projectreactor/netty/reactor-netty-core/1.1.5/*:$M2_REPO/io/netty/netty-handler-proxy/4.1.90.Final/*:$M2_REPO/io/netty/netty-codec-socks/4.1.90.Final/*:$M2_REPO/org/springframework/spring-web/6.0.7/*:$M2_REPO/org/springframework/spring-beans/6.0.7/*:$M2_REPO/org/springframework/spring-webflux/6.0.7/*:$M2_REPO/io/projectreactor/reactor-core/3.5.4/*:$M2_REPO/org/reactivestreams/reactive-streams/1.0.4/*:$M2_REPO/org/springframework/boot/spring-boot-starter-web/3.0.5/*:$M2_REPO/org/springframework/spring-aop/6.0.7/*:$M2_REPO/org/springframework/spring-expression/6.0.7/*:$M2_REPO/org/springframework/spring-context/6.0.7/*:$M2_REPO/org/springframework/boot/spring-boot-starter-validation/3.0.5/*:$M2_REPO/org/hibernate/validator/hibernate-validator/8.0.0.Final/*:$M2_REPO/jakarta/validation/jakarta.validation-api/3.0.2/*:$M2_REPO/org/jboss/logging/jboss-logging/3.5.0.Final/*:$M2_REPO/com/fasterxml/classmate/1.5.1/*:$M2_REPO/org/springframework/boot/spring-boot-starter-actuator/3.0.5/*:$M2_REPO/org/springframework/boot/spring-boot-actuator-autoconfigure/3.0.5/*:$M2_REPO/org/springframework/boot/spring-boot-actuator/3.0.5/*:$M2_REPO/io/micrometer/micrometer-observation/1.10.5/*:$M2_REPO/io/micrometer/micrometer-commons/1.10.5/*:$M2_REPO/io/micrometer/micrometer-core/1.10.5/*:$M2_REPO/org/hdrhistogram/HdrHistogram/2.1.12/*:$M2_REPO/org/latencyutils/LatencyUtils/2.0.3/*:$M2_REPO/org/slf4j/slf4j-api/2.0.7/*:$M2_REPO/org/springframework/spring-core/6.0.7/*:$M2_REPO/org/springframework/spring-jcl/6.0.7/*"

[packager]
language = "java"

[packager.features]
packageSearch = true

[languages.java]
pattern = "src/main/java/**/*.java"

[languages.java.languageServer]
start = "jdt-language-server"

[unitTest]
language = "java"

[nix]
channel = "stable-22_11"

[debugger]
support = true

[debugger.compile]
command = "javac -classpath .:/run_dir/junit-4.12.jar:$CLASSPATH:target/dependency/* -g -d target/classes $(find . -type f -name '*.java')"

[debugger.interactive]
transport = "localhost:0"
connectTimeout = 60
startCommand = "java-debug"

[debugger.interactive.initializeMessage]
command = "initialize"
type = "request"

[debugger.interactive.initializeMessage.arguments]
adapterID = "cppdbg"
clientID = "replit"
clientName = "replit.com"
columnsStartAt1 = true
linesStartAt1 = true
locale = "en-us"
pathFormat = "path"
supportsInvalidatedEvent = true
supportsProgressReporting = true
supportsRunInTerminalRequest = true
supportsVariablePaging = true
supportsVariableType = true

[debugger.interactive.launchMessage]
command = "launch"
type = "request"

[debugger.interactive.launchMessage.arguments]
classPaths = ["."]
mainClass = "com.replit.Application"

[deployment]
build = "mvn clean package -Dmaven.test.skip=true"
run = "java -jar target/classes/*.jar"